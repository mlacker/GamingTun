<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>

  <nlog throwConfigExceptions="true" xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target xsi:type="ColoredConsole" name="logconsole" layout="${time} ${pad:padding=-5:inner=${level:uppercase=true}}: ${message:exceptionSeparator=\n:withException=true}" useDefaultRowHighlightingRules="true">
        <highlight-row condition="level == LogLevel.Warn" foregroundColor="Yellow" />
        <highlight-row condition="level == LogLevel.Error" foregroundColor="Red" />
        <highlight-word regex="INFO" foregroundColor="DarkGreen" />
        <highlight-word regex="FATAL" foregroundColor="White" backgroundColor="Red" />
      </target>
    </targets>
    <rules>
      <logger name="*" writeTo="logconsole" />
    </rules>
  </nlog>

  <runtime>
